{
  "compilerOptions": {
    "target": "es2016",
    "lib": [
      "dom",
      "es2021"
    ],
    "module": "commonjs",
    "baseUrl": "./",
    },
    "isolatedModules": true,
    "esModuleInterop": true,
    "noImplicitAny": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "sourceMap": true,
    "strictNullChecks": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
  
  "include": [
    "src",
     ],
  "exclude": [
    "node_modules"
  ]
}
